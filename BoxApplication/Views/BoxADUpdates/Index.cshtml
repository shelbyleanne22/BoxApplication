@model IEnumerable<BoxApplication.Models.BoxADUpdate>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdateBoxOption)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ADFieldChanged)

            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateBoxOption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ADFieldChanged)
